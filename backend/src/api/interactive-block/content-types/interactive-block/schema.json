{
  "kind": "collectionType",
  "collectionName": "interactive_blocks",
  "info": {
    "singularName": "interactive-block",
    "pluralName": "interactive-blocks",
    "displayName": "Interactive Block",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "sounds": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "interactive_block_id": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "rich_text_caption": {
      "type": "richtext"
    },
    "chapters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chapter.chapter",
      "mappedBy": "interactive_blocks"
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "interactive-with-image",
        "interactive-with-video",
        "interactive-split-screen"
      ],
      "default": "interactive-with-image"
    },
    "soundtracks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::soundtrack.soundtrack"
    }
  }
}
